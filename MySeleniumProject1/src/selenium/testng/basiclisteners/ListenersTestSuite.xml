<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="ListenersTestSuite">
	<listeners>
		<listener class-name="selenium.testng.basiclisteners.ListenersConfig" />
	</listeners>
	<test name="LoginTests">
		<classes>
			<class name="selenium.testng.basiclisteners.LoginTest">
				<methods>
					<include name="loginByEmail" />
					<include name="loginByFacebook" />
				</methods>
			</class>
		</classes>
	</test>
</suite>